{
    "version": 2,
    "builds": [
      {
        "src": "src/main.jsx",
        "use": "@vercel/vercel",
        "config": {}
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://threads-clone-nu-nine.vercel.app/api/$1",
        "methods": ["GET", "POST"]
      },
      {
        "src": "/(.*)",
        "dest": "/src/main.jsx"
      }
    ]
  }