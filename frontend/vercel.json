{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "https://threads-clone-nu-nine.vercel.app/$1", "methods": ["GET", "POST", "PUT", "DELETE"] },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  